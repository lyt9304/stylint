$size = 10px
$color = red

some-mixin($a, $b)
  font-size $a+$b

div
  .warning
    color red
  p
    color $color
    display inline-block
    translate translate(10px, 10px)

// @stylint off
p:active,
div.text
  padding -(15px-$size) 15px 20px 20px
  margin-left - $size
  margin-right -10px+15px
  some-mixin(10px, 2)
// @stylint on

div>p.warning
  background-image url(http://s.baixing.net/a.png)
  border-radius 20px+10px
  color rgba(100, 255, 255, 1) // @stylint ignore
  font-size 40px-30px+20px

a
  color red


